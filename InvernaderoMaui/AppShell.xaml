<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="InvernaderoMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:InvernaderoMaui.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="InvernaderoMaui">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:vistaPicker}"
        Route="vistaPicker" />

    <ShellContent
        Title="Detalles"
        ContentTemplate="{DataTemplate local:vistaDetalles}" 
        Route="vistaDetalles" />
</Shell>
